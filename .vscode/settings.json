{
    "cmake.autoSelectActiveFolder": true,
    "cmake.sourceDirectory": "${workspaceFolder}/SECLogger",
    "cmake.buildDirectory": "${workspaceFolder}/SECLogger/SimpleEasyCLogger/build",
    "cmake.buildArgs": [],
    "cmake.configureSettings": {
        "TARGET_GROUP": "Debug"
    },    
    "cmake.buildBeforeRun": true,


    "editor.formatOnSave": true,
    "editor.formatOnType": false,


    "editor.defaultFormatter": "xaver.clang-format",
    "clang-format.executable": "/usr/bin/clang-format",
    "clang-tidy.executable": "/usr/bin/clang-tidy",
    "clang-tidy.fixOnSave": true,
    "clang-tidy.compilerArgs": [
        "-ISECLogger/SimpleEasyCLogger/inc"
    ],

    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.autocomplete": "Default",

    "doxdocgen.c.firstLine": "/*!",
    "doxdocgen.c.triggerSequence": "/*!",
    "doxdocgen.cpp.tparamTemplate": "\\tparam {param} ",
    "doxdocgen.file.copyrightTag": [
        "\\copyright Rete Ferroviaria Italiana S.p.A."
    ],

    "doxdocgen.file.fileOrder": [
        "file",
        "brief",
        "author",
        "custom",
        "version",
        "date",
        "copyright",
    ],
    "doxdocgen.file.fileTemplate": "\\name {name}",
    "doxdocgen.file.versionTag": "\\version 0.1",
    "doxdocgen.file.customTag": [
        "\\details",
        "\\pre",
        "\\bug",
        "\\warning",
        "\\note"
    ],
    "doxdocgen.generic.authorTag": "\\author {author} ({email})",
    "doxdocgen.generic.boolReturnsTrueFalse": true,
    "doxdocgen.generic.briefTemplate": "\\brief",
    "doxdocgen.generic.dateFormat": "YYYY-MM-DD",
    "doxdocgen.generic.dateTemplate": "\\date {date}",
    "doxdocgen.generic.generateSmartText": true,
    "doxdocgen.generic.includeTypeAtReturn": true,
    "doxdocgen.generic.order": [
        "brief",
        "custom",
        "empty",
        "tparam",
        "param",
        "return",
    ],
    "doxdocgen.generic.customTags": [
        "\\details <info>."
    ],
    "doxdocgen.generic.paramTemplate": "\\param[in]{indent:18}{param}:{indent:37}<Info>.",
    "doxdocgen.generic.returnTemplate": "\\return{indent:18}{type}:{indent:37}<Info>.",
    "doxdocgen.generic.useGitUserName": true,
    "doxdocgen.generic.useGitUserEmail": true,
    
    "files.associations": {
        "*.td": "c",
        "*.def": "c",
        "*.h": "c",
        "cstdlib": "c",
        "cstdio": "c",
        "array": "c",
        "string_view": "c",
        "initializer_list": "c",
        "utility": "c",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "ostream": "cpp",
        "semaphore": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "stop_token": "cpp",
        "streambuf": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "typeinfo": "cpp",
        "*.hpp": "cpp",
        "*.h++": "cpp",
        "unordered_set": "cpp",
        "optional": "cpp",
        "mutex": "cpp"
    },
}